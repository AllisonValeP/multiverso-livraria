<%- include('./partials/head')%>
<%- include('./partials/header')%>
<section>
  <ul class="banner grid responsive">
    <li><img class="banner-image" src="/img/livroBanner-01-teste.png" alt="Flowers in Chania"></li>
    <li><img class="banner-image" src="/img/livroBanner-02-teste.png" alt="Flowers in Chania"></li>
